{
  "expo": {
    "name": "DasChat",
    "slug": "daschat",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/photo/logoDas.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFF00"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/photo/chat.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.dasrnapps.daschat"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "17a54b94-951c-4dc9-8866-fb175bcc90b4"
      }
    }
  }
}
